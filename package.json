{
  "name": "back-end",
  "version": "1.0.0",
  "description": "Back-end for Syntrest - A full stack MERN app",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node -r dotenv/config server.js",
    "server": "nodemon -r dotenv/config server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Jake Riordan",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.15",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.2.1",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "passport-jwt": "^4.0.0",
    "unsplash-js": "^5.0.0",
    "validator": "^11.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
